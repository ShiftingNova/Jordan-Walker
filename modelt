length= int(input("Model-T length:\n"))
print(".-----------"+"-"*length+".")
print("| ### ||  ###" + "#" * length + "\ ")
print("| ### ||  ####"+"#"*length+"\.")
print("D     ||  "+" "*length+"<>    |------+")
print("|  ______      "+" "*length+"/______ |")
print(" \/ /..\ \_____"+"_"*length+"/ /..\ \|")
print("    \__/         "+" "*length+"\__/")
